/* On crée les trois fonctions COOKIES */


function createCookie (name,valeur_Navi)
{
var expire_Navi="";

document.cookie=name+"="+valeur_Navi+expire_Navi+"; path=/";               /* On crée un cookie sur la machine de l'utilisateur */		
}

function readCookie (name)													/* Je découpe la chaine de caractère du COOKIE, en deux champs */		
{
const name_Navi=name+"=";
const cookie_Navi=document.cookie.split (";");
for(let i=0; i<cookie_Navi.length; i=i+1)                     				/* L'index "i" va parcourir tous les cookies préparée dans les instructions précédentes */
{
while (cookie_Navi [i].charAt(0)===" ")										/* Supprime les espaces inutiles dans les cookies (il modifie les COOKIES*/
{cookie_Navi [i]=cookie_Navi [i].substring(1,cookie_Navi [i].length);
}
if (cookie_Navi[i].indexOf(name_Navi)===0) return cookie_Navi [i].substring(name_Navi.length,cookie_Navi [i].length);
}
return null;
}

function eraseCookie (name)
{
createCookie(name,"",-1);
}









